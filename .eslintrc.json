{
    "plugins": ["@typescript-eslint"],
    "extends": ["prettier", "eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "no-duplicate-imports": "error",
      "no-nested-ternary": "warn",
      "curly": "error"
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  }
  